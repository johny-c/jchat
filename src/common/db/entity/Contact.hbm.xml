<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="common.db.entity">
  <class name="Contact" table="CONTACTS">
    <id column="C_ID" name="id" type="integer">
      <generator class="assigned"/>
    </id>
    <property column="C_USERNAME" name="username" type="string"/>
    <property column="C_EMAIL" name="email" type="string"/>
    <property column="C_STATUS" name="status"/>
    <!-- <property column="C_CHAT_OPENED" name="chatPaneOpened" type="boolean" />
        <property column="C_VIDEO_OPENED" name="videoPaneOpened" type="boolean" /> -->
  </class>
</hibernate-mapping>
